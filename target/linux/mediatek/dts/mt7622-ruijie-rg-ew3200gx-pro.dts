/dts-v1/;
#include "mt7622-ruijie-rg-ew3200.dtsi"

/ {
	model = "Ruijie RG-EW3200GX PRO";
	compatible = "ruijie,rg-ew3200gx-pro", "mediatek,mt7622";
};

&nor_flash {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&spi_nor_pins>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@00000 {
				label = "Preloader";
				reg = <0x00 0x20000>;
			};

			partition@40000 {
				label = "ATF";
				reg = <0x20000 0x10000>;
			};

			partition@60000 {
				label = "u-boot";
				reg = <0x30000 0x50000>;
			};

			partition@B0000 {
				label = "u-boot-env";
				reg = <0x80000 0x10000>;
			};

			partition@D0000 {
				label = "Factory";
				reg = <0x90000 0x40000>;
			};

			partition@150000 {
				label = "product_info";
				reg = <0xd0000 0x10000>;
			};

			partition@160000 {
				label = "kdump";
				reg = <0xe0000 0x10000>;
			};

			partition@170000 {
				label = "firmware";
				reg = <0xf0000 0xf10000>;
			};
		};
	};
};

&wmac {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&epa_elna_pins>;
	mediatek,mtd-eeprom = <&factory 0x0>;
};

&slot0 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x5000>;
		ieee80211-freq-limit = <5000000 6000000>;
		mediatek,disable-radar-background;
	};
};
